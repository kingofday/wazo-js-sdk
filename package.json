    "jsrsasign": "^11.1.0",
    "libphonenumber-js": "^1.10.60",
    "moment": "^2.30.1",
    "reconnecting-websocket": "^4.4.0",
    "sdp-transform": "^2.14.2",
    "sip.js": "^0.21.2",
    "webrtc-adapter": "^8.2.3"
  },
  "devDependencies": {
